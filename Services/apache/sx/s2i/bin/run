#!/bin/bash -e
#
# S2I run script
# The 'run' script is the entrypoint of your application
#
# For more information refer to the documentation:
#  https://github.com/openshift/source-to-image/blob/fc28/docs/builder_image.md
#

if [[ -x /tmp/run ]]; then
    exec /tmp/run $@
else
    exec /bin/sx-apache run
fi 
#!/bin/bash -e
#
# S2I run script for the 'startx/sv-chrome' image.
# The 'run' script is the entrypoint of your application
#
# For more information refer to the documentation:
#  https://dockerhub.com/u/startx/sv-chrome
#

if [[ -x /tmp/run ]]; then
    exec /tmp/run $@
else
    exec /bin/sx-chrome run
fi 